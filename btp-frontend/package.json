{
    "name": "btp-frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:docker": "ng serve --host 0.0.0.0 --disable-host-check --poll 500",
        "build:dev": "ng build",
        "build:prod": "ng build --prod",
        "build:local": "ng build -c prod-local",
        "postbuild:dev": "cp -r build ../backend/",
        "postbuild:prod": "cp -r build ../backend/",
        "postbuild:local": "cp -r build ../backend/",
        "build-css": "node-sass src/assets/custom-themes/ -o src/assets/out-themes/",
        "watch-css": "nodemon -e scss -x \"npm run build-css\"",
        "postinstall": "ngcc",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.4",
        "@angular/cdk": "^11.0.2",
        "@angular/common": "~11.0.4",
        "@angular/compiler": "~11.0.4",
        "@angular/core": "~11.0.4",
        "@angular/forms": "~11.0.4",
        "@angular/material": "^11.0.2",
        "@angular/platform-browser": "~11.0.4",
        "@angular/platform-browser-dynamic": "~11.0.4",
        "@angular/router": "~11.0.4",
        "@ncstate/sat-popover": "^7.1.0",
        "angularx-social-login": "^3.5.4",
        "file-saver": "^2.0.5",
        "jquery": "^3.5.1",
        "moment": "^2.29.1",
        "node-sass": "^5.0.0",
        "rxjs": "^6.6.3",
        "t-writer.js": "^1.0.4",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.4",
        "@angular/cli": "~11.0.4",
        "@angular/compiler-cli": "~11.0.4",
        "@angular/language-service": "~11.0.4",
        "@babel/compat-data": "7.8.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.4",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.5"
    }
}